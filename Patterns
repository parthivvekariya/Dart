 4321
54321

import 'dart:io';

void main()
{
  int n = 5 ,i,j;

  for(i=1; i<=n ; i++)
  {
    for(j=i ; j<n ; j++)
    {
      stdout.write(" ");
    }
    for(j=i ; j>=1 ; j--)
    {
      stdout.write(j);
    }

    stdout.writeln();
  }
}
